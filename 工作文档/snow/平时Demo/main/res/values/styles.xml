<?xml version="1.0" encoding="utf-8"?>
<resources>


    <style name="Dialog_FS">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <declare-styleable name="MaxHeightScrollView">
        <attr name="maxHeight" format="dimension" />
    </declare-styleable>

    <declare-styleable name="MaxWidthLinearLayout">
        <attr name="maxWidth" format="dimension" />
    </declare-styleable>

    <!--    multi_image_selector-->
    <style name="MIS_NO_ACTIONBAR" parent="Theme.AppCompat.Light" />

    <style name="MIS_EDIT_IMAGE_THEME" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>


    <style name="dialog_style" parent="@android:style/Theme.Dialog">
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:background">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
    </style>

    <declare-styleable name="PingFangTextView">
        <attr format="enum" name="pingfang_weight">
            <enum name="regular" value="0"/>
            <enum name="medium" value="1"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="DesignTextView">
        <attr name="dtv_text_style" format="enum">
            <enum name="regular" value="0" />
            <enum name="medium" value="1" />
            <enum name="bold" value="2" />
        </attr>

        <attr name="dtv_font" format="enum">
            <enum name="normal" value="0" />
            <enum name="din" value="1" />
            <enum name="auto" value="2" />
        </attr>

        <attr name="dtv_resize" format="boolean" />
        <attr name="dtv_height_optimize" format="boolean" />
        <attr name="dtv_is_feed" format="boolean" />
        <attr name="dtv_min_textsize" format="dimension|reference" />
    </declare-styleable>

    <style name="Theme.MyApp.Light" parent="Design.Theme.Light">

    </style>

    <style name="Theme.MyApp.Dark" parent="Design.Theme.Night">
    </style>



</resources>